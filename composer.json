{
    "name": "laravie/authen",
    "description": "User Authentication Identifiers for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Authen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Authen\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/auth": "~5.3"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.6 || ~3.4.5 || ~3.5",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "~5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
