{
    "name": "laravie/authen",
    "description": "User Authentication Identifiers for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\Authen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\Authen\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0",
        "illuminate/auth": "~5.2.0 || ~5.3.0 || ~5.4.0 || ~5.5.0"
    },
    "require-dev": {
        "illuminate/database": "^5.2",
        "orchestra/database": "~3.2",
        "orchestra/testbench": "~3.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
